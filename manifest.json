{
  "name": "Draft",
  "version": "1",
  "manifest_version": 2,
  "description": "__MSG_appDescription__",
  "icons": {
    "16": "icon-16.png",
    "128": "icon-128.png"
  },
  "default_locale": "en",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "background": { "scripts": ["background.js", "jquery.js"] },
  "permissions": [
    "cookies",
    "http://*/*", 
    "tabs"
  ], 
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["content_script.js"]
    }
  ]
}
